package yesterday;

public class BasicData {

	public static void main(String[] args) {
		//자동완성: 컨트롤+스페이스바
		//sysout, syso
		System.out.println("자바 두번째 날입니다.");
		//컨트롤+s
		//컨트롤+f11
		//num = 100;
		//기본데이터: 정수, 실수, 문자1, 논리
		//변수(저장공간), 변수명
		//자바에서의 저장공간 생성 시기
		int num; //들어갈데이터타입 변수명; 선언!
		//선언을 할 때 자바는 변수가 할당, 4바이트(-21억~21억)
		
		double eye; //8바이트
		
		char gender; //2바이트
		
		boolean food; //1바이트
		
		System.out.println(num);
		
		
		
		
		
	}

}
